%h2= @page_title = "Log in"
.span-9.colborder
  - form_for @user_session do |f|
    %p
      = f.label :email, "Email address"
      %br
      = f.text_field :email
    %p
      = f.label :password
      %br
      = f.password_field :password
    %p= f.submit "Log in"

.span-14.last
  %p= link_to "Forgot your password?", new_password_reset_path